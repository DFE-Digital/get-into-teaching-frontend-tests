{
	"baseUrl": "https://get-into-teaching-app-test.london.cloudapps.digital",
	"env": {
		"TTA_ROOT_URL": "https://get-teacher-training-adviser-service-test.london.cloudapps.digital/"
	},
	"chromeWebSecurity": false,
	"defaultCommandTimeout": 15000,
	"pageLoadTimeout": 15000,
	"reporter": "cypress-multi-reporters",
	"reporterOptions": {
		"reporterEnabled": "mochawesome",
		"mochawesomeReporterOptions": {
			"reportDir": "cypress/reports",
			"reportFilename": "testreport",
			"quite": true,
			"overwrite": false,
			"html": false,
			"json": true
		}
	},
	"retries": 2
}
